    "graphql": "^0.13.2",
    "jsonwebtoken": "^8.3.0",
    "merge-graphql-schemas": "^1.5.2",
    "mongoose": "^5.1.7",
    "mongoose-autopopulate": "^0.8.0",
    "mongoose-sequence": "^4.0.1",
    "passport": "^0.4.0",
